<Window x:Class="gazdalkodjOkosan.Vegkiiras"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:gazdalkodjOkosan"
        mc:Ignorable="d"
        Title="Vegkiiras" Height="450" Width="800" FontSize="40">
    <Grid>
        <Grid.Background>
            <ImageBrush/>
        </Grid.Background>
        <Label Content="Gratulálunk ... a győztes!" HorizontalAlignment="Center" Height="74" Margin="0,20,0,0" VerticalAlignment="Top" Background="#FFF7C556" Panel.ZIndex="1"/>
        <Button Name="btnKilepes" Width="120px" Height="50px" Margin="550 300 0 0" Content="Kilépés" FontSize="20px" Click="btnKilepes_Click" Panel.ZIndex="1" Background="#FFF7C556"/>
        <Image Source="/images/gyoztes.jfif" Stretch="Fill"/>
    </Grid>
</Window>
